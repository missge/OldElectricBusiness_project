body{  background: #F7F7F7;}
/* 列表页公共的*/
.OL_content{
    background: #F7F7F7;
    position: absolute;
    top:38px;
    bottom:0px;
    width: 100%;
  }
  .OL_title{
    height: 32px;
    line-height: 32px;
  }
  .OL_title>.lf{
    color: #3275BC;
    font-size: 0.6rem;
    line-height: 30px;
  }
  .OL_title>.lf i{
    display: inline-block;
       width: 12px;
    height: 30px;
    background: url("../img/Or_icon.png") no-repeat left center;
    background-size: 100%;
    vertical-align: bottom;
    margin-right: 5px;
    margin-left: 5px;
  }
  .OL_title>.rt{
   font-size: 0.65rem;
    color:#434343;
    padding-right: 10px;
  }
  .OL_fooditem{
    display: flex;
    /*position: relative;*/
  }
  .OL_fooditem_lf{
    flex:0 0 80px;
    width: 80px;

  }
  .OL_foodltem_rt{
  -webkit-box-flex: 1;
    -ms-flex: 1;
    flex: 1;
    /* top: 50%; */
    /* left: 100px; */
    /* transform: translateY(-50%); */
    position: relative;
    margin-left: 10px;
    // line-height: 72px;
        padding: 6px 0px;
  }
  .OL_fooditem_lf>img{
    margin:10px 0; 
  }
  .OL_foodltem_rt>span:first-child{
    font-size: 0.65rem;

  }
  .OL_foodltem_rt>span:last-child{
       font-size: 0.65rem;
    font-weight: bold;
    position: absolute;
    right: 10px;
    top: 50%;
    transform: translateX(0) translateY(-50%);
        text-align: right;

  }
  .mar_top_bot{
    display: flex;
    height:48px;
    line-height: 48px;
  }
  .mar_top_botrt{
    line-height: 36px;
    padding-top: 5px;
  }
  .mar_top_botlf,.mar_top_botrt{
    flex:1;
    font-size: 0.65rem;
  }
  .mar_top_botlf span{
 font-size: 0.65rem;
  }
  .mar_top_botrt {
    text-align: right;
  }
 
  /*点餐订单详情-餐厅扫码后展示*/
  .ODP_header{
    margin-top: 8px;
    background: #fff;
    text-align: center;
  }
  .ODP_header>img{
    padding: 15px 0; 
  }
  .ODP_title{
    padding-bottom: 22px;
    color: #3275B9;
    font-size: 1rem;
    font-weight: bold;
  }
  .ODP_Message{
    height: 50px;
    line-height: 50px;
  }
  .width_auto{
   font-size: 0.8rem;
    color: #434343;
  }
  .width_auto>i{
    display: inline-block;
    width: 25px;
    height: 20px;
    background:url('../img/messge.png') no-repeat center center;
    background-size:100%;
    vertical-align: middle;
    margin:0px 10px;
  }
  /*点餐订单详情-已收货*/
  .ODG_title{
    height: 40px;
    line-height: 40px;
    background: #fff;
    padding-left:17px;
    font-size: 0.8rem; 
    color:#434343;
  }
  .pub_messge{
     height: 50px;
       line-height: 50px;
       text-align: left;
  }
  .pub_messge>.width_auto{border-top:1px solid #DCDCDC;}
  .bold{font-weight: bold;color: #1C1C1C}
  .red{color:#F74945;}
  .blue{color:#5B98CE;}



  /*底部*/
.shopCart {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 48px;
    z-index: 50;
}
.Online_content {
    display: -ms-flexbox;
    display: flex;
    background: #D8D8D8;
}
.Online_content-left{
  -ms-flex: 1;
    flex: 1;
    height: 50px;
}
.Online_content-right{
      background: #416EC2;
    color: #fff;
        flex: 0 0 122px;
    font-size: 20px;
    font-weight: 700;
    line-height: 48px;
    text-align: center;
}

/*加减号*/
.cartcontrol-wrapper {
    position: absolute;
    right: 0;
    bottom: 12px;
    z-index: 20;
}
.cartcontrol .cart-decrease {
    display: inline-block;
    padding: 6px 0;
}
.cart-decrease{
  transition: all 0.4s linear;
}
.cart-add{
  display: inline-block;
    vertical-align: top;
    font-size: 24px;
    color: #3375BB;
    line-height: 24px;
    padding: 6px 0;
}
 .inner {
    line-height: 24px;
    font-size: 24px;
    color: #539AD4;
    transition: all 0.4s linear;
    display: block;
}
.cart-count {
    display: inline-block;
    vertical-align: top;
    font-size: 0.7rem;
    color: #93999f;
    line-height: 24px;
    text-align: center;
    padding: 6px 0;
}
.Online_Surplus{
  font-size:0.6rem;
  color:#838383;
  margin: 2px 0 8px 0;
}
